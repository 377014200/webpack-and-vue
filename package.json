{
    "name": "webpack-and-vue",
    "version": "1.0.0",
    "description": "告诉你个秘密: 我是无敌的",
    "sideEffects": [
        "*.css",
        "*.vue"
    ],
    "private": true,
    "directories": {
        "test": "test"
    },
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "cross-env NODE_ENV=production node build/build.js",
        "build:report": "cross-env NODE_ENV=production report=true node build/build.js",
        "dll": "cross-env NODE_ENV=production webpack --progress --config build/webpack.dll.js",
        "dev-bundle": "webpack --profile --config bundle/webpack.dev.js",
        "dev-profile-bundle": "webpack --profile --json > stats.json --config build/webpack.dev.js",
        "profile-prod": "webpack --json >stats.json --config build/webpack.prod.js",
        "server": "cross-env NODE_ENV=development webpack-dev-server --config build/server.js",
        "lint-fix": "eslint --fix --ext .js,.vue src",
        "test:unit": "jest tests/unit --notify"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "devDependencies": {
        "@babel/core": "^7.5.0",
